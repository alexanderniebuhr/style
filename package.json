  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexanderniebuhr/style.git"
  },
  "license": "MIT",
  "author": "",
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sort": "npx sort-package-json@latest",
    "sort:all": "npm run sort && npm run sort --workspaces",
    "patch:all": "npx @jsdevtools/version-bump-prompt@latest patch && npx --workspaces @jsdevtools/version-bump-prompt@latest patch",
    "minor:all": "npx @jsdevtools/version-bump-prompt@latest minor && npx --workspaces @jsdevtools/version-bump-prompt@latest minor",
    "major:all": "npx @jsdevtools/version-bump-prompt@latest major && npx --workspaces @jsdevtools/version-bump-prompt@latest major",
    "publish:all": "npm publish --tag=latest && npm publish --tag=latest --workspaces"
  },
  "peerDependencies": {
    "@alexanderniebuhr/eslint-config": "*",
    "@alexanderniebuhr/eslint-plugin": "*",
    "@alexanderniebuhr/prettier-config": "*"
  },
  "volta": {
    "npm": "7.21.1",
    "node": "16.8.0"
  }
}
